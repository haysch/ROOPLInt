class Foo
    method addOne(int y)
        y += 1

    method addTwo(int u)
        call addOne(u)
        call addOne(u)

class Program
    int z
    Foo f
    
    method main()
        new Foo f
        local int x = 0
        call f::addTwo(x)
        z += x
        uncall f::addTwo(x)
        delocal int x = 0
